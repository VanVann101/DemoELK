{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": []
  },
  "InventoryService": {
    "BaseUrl": "http://inventory-service"
  },
  "PaymentService": {
    "BaseUrl": "http://payment-service"
  },
  "Logstash": {
    "Host": "logstash",
    "Port": 5000
  }
}

